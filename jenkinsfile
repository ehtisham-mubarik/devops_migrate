pipeline{
  agent any
  
   stages {
     
        stage ('install_stage')
          steps {
                           echo 'first jenkins '
                        sh 'npm i'
                 
                 
            }
            stage ('start_roject')
            steps {
                        echo 'project'
                        sh 'npm start'
                 
                 
            }
            
          }
     }
            
